alias at='alacritty-themes'

# git alias'
alias gs='git status'
alias ga='git add .'
alias gcm='git commit -m'
alias gp='git push -u origin'

# shortcuts
alias home='cd ~'
alias code='cd ~/workspace'
alias config='cd ~/.config'

alias ls='ls -FA --color --group-directories-first'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
