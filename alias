alias at='alacritty-themes'

# git alias'
alias gs='git status'
alias ga='git add .'
alias gcm='git commit -m'
alias gp='git push -u origin'

# shortcuts
alias home='cd $HOME/'
alias codez='cd $HOME/workspace'
alias config='cd $HOME/.config'

alias ls='ls -a1F --color --group-directories-first'

alias profile='source .profile'
alias newAlias='nano $HOME/dootfiles/alias'
